<torc>
  <inputs>
    <network>
      <switch>
        <name>test</name>
        <default>0</default>
      </switch>
    </network>
  </inputs>
  <outputs>
    <network>
      <switch>
        <name>testoutput</name>
        <default>0</default>
      </switch>
    </network>
  </outputs>
  <notify>
    <notifiers>
      <log>
        <name>log</name>
      </log>
    </notifiers>
    <notifications>
      <system>
        <name>system</name>
        <inputs>
          <event>start</event>
        </inputs>
        <outputs>
          <notifier>testoutput</notifier>
        </outputs>
        <message><body>hello</body></message>
      </system>
    </notifications>
  </notify>
</torc>
